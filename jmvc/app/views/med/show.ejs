

<td><%= this.name_title %></td>
<td><%= this.quantity_value %> <%= this.quantity_unit %></td>
<td><%= this.frequency_value %> <%=this.frequency_unit %></td>
<td><%= this.instructions %></td>
<td><%= this.startDate %></td>
<td><%= this.endDate %></td>
<% this.fulfillments.sort(function(a,b) {
    	return (a.date > b.date) ? -1 : (a.date < b.date) ? 1 : 0;});%>
<td>
	<% if (this.fulfillments && this.fulfillments.length > 0) { %>       
		<%= this.fulfillments[0].date %>
	<% } %>
</td>