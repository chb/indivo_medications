<h2>Medications</h2>

<table class="light">
    <thead>
    <tr>
      <th>name</th>
      <th>dose</th>
      <th>frequency</th>
      <th>instructions</th>
      <th>started on</th>
      <th>stopped on</th>
      <th>last filled</th>
      <!-- <th>Options</th> -->
    </tr>
    </thead>
    <tbody>
        <%= view('./views/med/list',{meds: meds})%>
    </tbody>
</table>
